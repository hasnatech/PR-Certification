                        "showMenu": true,
                        "interaction": false,
                        "start_num":4,
                        "block": [{
                                "text": "Labor should be expensed separately from materials, regardless of the service being quoted or estimated.",
                                "time": "1"
                            },
                            {
                                "text": "Item category will always be “D” to denote Service. Remember, Item category D is not used for integrated suppliers.",
                                "time": "8.7"
                            },
                            {
                                "text": "A Service Master number is required for all services.",
                                "time": "19.9"
                            },
                            {
                                "text": "Short text description should be descriptive enough for PO Processor and Supplier to understand the service needs. Additional text can be expanded in the Item Text field.",
                                "time": "25.6"
                            },
                            {
                                "text": "Services can be 3-way match (PO/GR/IR) or a 2-way (PO/IR) match.",
                                "time": "39.3"
                            }

                        ],
